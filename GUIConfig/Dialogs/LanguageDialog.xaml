<Window x:Class="GUIConfig.Dialogs.LanguageDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Choose Language" Height="133" Width="220" ResizeMode="NoResize" WindowStyle="ThreeDBorderWindow" Name="UI">
	<Grid>
		<Button Content="OK" Height="23" HorizontalAlignment="Right" Margin="0,0,12,12" Name="ButtonOk" VerticalAlignment="Bottom" Width="75" Click="Button_OK_Click" />
		<ComboBox ItemsSource="{Binding ElementName=UI, Path=Languages}" SelectedItem="{Binding ElementName=UI, Path=SelectedLanguage}" Height="23" Margin="12,26,12,0" Name="ComboBox1" VerticalAlignment="Top" />
		<TextBlock Height="23" HorizontalAlignment="Left" Margin="12,10,0,0" Name="TextBlock1" Text="Languages..." VerticalAlignment="Top" />
	</Grid>
</Window>
