<UserControl x:Class="GUISkinFramework.Editors.ImageComboEditor"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:imageEditor="clr-namespace:GUISkinFramework.Editors"
             mc:Ignorable="d" 
             Name="UI" Width="Auto">
    <UserControl.Resources>
     
    </UserControl.Resources>

    <imageEditor:ImagePickerCombo x:Name="Picker" SkinInfo="{Binding SkinInfo, ElementName=UI}" SelectedImage="{Binding SelectedImage, ElementName=UI, Mode=TwoWay}" />
</UserControl>
