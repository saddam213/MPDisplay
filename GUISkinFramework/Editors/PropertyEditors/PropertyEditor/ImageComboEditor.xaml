<UserControl x:Class="GUISkinFramework.Editor.PropertyEditors.ImageComboEditor"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:chrome="clr-namespace:MPDisplay.Common.Controls.Chromes;assembly=MPDisplay.Common"
             xmlns:Converters="clr-namespace:GUISkinFramework.Converters"
             xmlns:editors="clr-namespace:GUISkinFramework.Editor.PropertyEditors.PropertyEditor"
             mc:Ignorable="d" 
             Name="UI" Width="Auto">
    <UserControl.Resources>
     
    </UserControl.Resources>

    <editors:ImagePickerCombo x:Name="picker" SkinInfo="{Binding SkinInfo, ElementName=UI}" SelectedImage="{Binding SelectedImage, ElementName=UI, Mode=TwoWay}" />
</UserControl>
