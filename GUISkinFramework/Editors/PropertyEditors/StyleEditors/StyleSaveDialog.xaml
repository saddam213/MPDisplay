<Window x:Class="GUISkinFramework.Editors.StyleSaveDialog"
    
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
     
        Title="StyleSaveDialog" Height="129" Width="300" Name="UI" WindowStartupLocation="CenterOwner">
    <Grid>
        <Button Content="Save" HorizontalAlignment="Right" Margin="0,0,90,10" VerticalAlignment="Bottom" Width="75" Click="Button_Save_Click"/>
        <Button Content="Cancel" HorizontalAlignment="Right" Margin="0,0,10,10" VerticalAlignment="Bottom" Width="75" Click="Button_Cancel_Click"/>
        <StackPanel Margin="10,10,10,39">
            <TextBlock TextWrapping="Wrap" Text="StyleId"/>
            <TextBox Height="23"  Text="{Binding ElementName=UI, Path=StyleId}"/>
        </StackPanel>

    </Grid>
</Window>
